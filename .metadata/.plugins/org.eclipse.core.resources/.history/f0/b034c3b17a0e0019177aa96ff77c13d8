<?xml version="1.0" encoding="UTF-8"?>
<template name="renewSubscriptionResponseTemplate" xmlns="http://ws.apache.org/ns/synapse">
    <parameter name="transactionid"/>
    <parameter name="status"/>
    <parameter name="price"/>
    <parameter name="errorCode"/>
    <parameter name="errorMessage"/>
    <sequence>
        <payloadFactory media-type="json">
            <format>{“data”:
				{
				“aocTransID”: “$1”,
				“transactionOperationStatus”: “$2”,
				“totalAmountCharged”: “$3”,
				“errorCode”: “$4”,
				“errorMessage”: “$5”
				}
				}</format>
            <args>
                <arg evaluator="xml" expression="$func:transactionid"/>
                <arg evaluator="xml" expression="$func:status"/>
                <arg evaluator="xml" expression="$func:price"/>
                <arg evaluator="xml" expression="$func:errorCode"/>
                <arg evaluator="xml" expression="$func:errorMessage"/>
            </args>
        </payloadFactory>
    </sequence>
</template>
